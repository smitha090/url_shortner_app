<p style='float: right;'>
  <%= link_to "Go Back", request.referer %>
<p>  
<h1> Analytics </h1>
<hr>
<% @links.each do |link| %>
  <p>Original URL :
    <%= link.original_url %>
  </p> 
  <p>Short URL :
    <%= "https://127.0.0.1:300/" + link.short_url %>
  </p> 
  </br>
  <p>Total Clicks :
    <%= link.clicked %>
  </p>
  <p> IP:
    <%= @ip_address %>
  <p> Countries:
    <%= @countries %>
  </p>  
  </br> 
  <hr>
 <%end%> 

